{
   "name": "safe-security-task",
   "version": "0.0.0",
   "private": true,
   "scripts": {
      "watch-ts": "tsc -w",
      "watch-node": "NODE_ENV=dev nodemon dist/server.js",
      "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
      "serve-debug": "NODE_ENV=dev nodemon --inspect dist/server.js",
      "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
      "build-ts": "tsc",
      "tslint": "tslint -c tslint.json -p tsconfig.json",
      "copy-static-assets": "ts-node copy-static-assets.ts",
      "copy-static-assets-prod": "ts-node copy-static-assets.prod.ts",
      "build": "npm run clean && npm run build-ts && npm run tslint && npm run copy-static-assets",
      "build-prod": "npm run build-ts-prod && npm run tslint && npm run copy-static-assets",
      "debug": "npm run build && npm run watch-debug",
      "serve": "node dist/server.js",
      "start": "npm run serve",
      "clean": "rm -rf dist"
   },
   "dependencies": {
      "@devslane/crypt-service-node": "^1.1.0",
      "@devslane/jwt-service-node": "^1.0.5",
      "@devslane/validator-service-node": "^1.1.1",
      "async": "^3.2.0",
      "body-parser": "^1.18.1",
      "compression": "^1.7.4",
      "cookie-parser": "~1.4.4",
      "cors": "^2.8.5",
      "crypto-js": "^4.0.0",
      "debug": "~2.6.9",
      "dotenv": "^10.0.0",
      "errorhandler": "^1.5.1",
      "express": "^4.15.4",
      "express-basic-auth": "^1.2.0",
      "http-errors": "~1.6.3",
      "lodash": "^4.17.21",
      "memjs": "^1.3.0",
      "moment": "^2.29.1",
      "mongodb": "^3.0.8",
      "morgan": "~1.9.1",
      "reflect-metadata": "^0.1.10",
      "server": "^1.0.33",
      "typeorm": "0.2.34",
      "valid-url": "^1.0.9"
   },
   "devDependencies": {
      "@types/async": "^3.2.6",
      "@types/cors": "^2.8.10",
      "@types/crypto-js": "^4.0.1",
      "@types/express": "4.17.1",
      "@types/express-serve-static-core": "4.17.1",
      "@types/lodash": "^4.14.170",
      "@types/memjs": "^1.2.2",
      "@types/mongodb": "^3.6.18",
      "@types/morgan": "^1.9.2",
      "@types/node": "^14.17.3",
      "@types/shelljs": "0.8.3",
      "concurrently": "^6.2.0",
      "nodemon": "^2.0.7",
      "shelljs": "0.8.3",
      "ts-node": "^8.4.1",
      "tslint": "^6.1.3",
      "typescript": "4.0.2"
   }
}
